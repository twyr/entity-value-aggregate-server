{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Entity Value Aggregate Server",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/servers/rest-api-server/source/index.js",
			"cwd": "${workspaceFolder}/servers/rest-api-server",
			"runtimeArgs": [],
			"env": {
				"NODE_ENV": "development"
			}
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Migrate Database",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/node_modules/.bin/knex",
			"cwd": "${workspaceFolder}/servers/rest-api-server/knex_migrations",
			"args": ["--knexfile", "./knexfile.cjs", "migrate:latest"],
			"env": {
				"NODE_ENV": "development"
			}
		}
		{
			"type": "node",
			"request": "launch",
			"name": "Seed Database",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/node_modules/.bin/knex",
			"cwd": "${workspaceFolder}/servers/rest-api-server/knex_migrations",
			"args": ["--knexfile", "./knexfile.cjs", "seed:run"],
			"env": {
				"NODE_ENV": "development"
			}
		}
	]
}
